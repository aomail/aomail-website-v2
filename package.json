{
  "name": "aomail-website",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "predev": "yarn gen:gql",
    "prebuild": "yarn run prettier --write 'src/**/*.ts' && yarn gen:gql",
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "check-types": "yarn tsc --noEmit",
    "gen:gql": "DOTENV_CONFIG_PATH=./.env.local graphql-codegen -r dotenv/config --config ./codegen.yml",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.12.9",
    "@graphql-codegen/cli": "^2.3.0",
    "@graphql-codegen/schema-ast": "^2.4.0",
    "@graphql-codegen/typescript": "^2.4.1",
    "@graphql-codegen/typescript-graphql-request": "^4.3.1",
    "@graphql-codegen/typescript-operations": "^2.2.1",
    "@hookform/resolvers": "^2.8.5",
    "@hubspot/api-client": "^5.0.0",
    "@stitches/react": "^1.2.5",
    "@svgr/webpack": "^5.5.0",
    "@types/google.maps": "^3.46.1",
    "@types/node": "^16.11.8",
    "@types/react": "^17.0.35",
    "@types/uuid": "^8.3.3",
    "autoprefixer": "^10.4.0",
    "datocms-html-to-structured-text": "^1.2.0",
    "datocms-structured-text-utils": "^1.2.0",
    "framer-motion": "^5.3.0",
    "graphql": "^16.0.1",
    "graphql-request": "^3.6.1",
    "graphql-tag": "^2.12.6",
    "isomorphic-unfetch": "^3.1.0",
    "little-state-machine": "^4.2.0",
    "next": "latest",
    "next-compose-plugins": "^2.2.1",
    "parse5": "^6.0.1",
    "public-ip": "^4.0.4",
    "react": "^17.0.2",
    "react-datocms": "^1.6.6",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.22.0",
    "react-text-mask": "^5.4.3",
    "rehype-raw": "^6.1.0",
    "rehype-stringify": "^9.0.2",
    "remark-breaks": "^3.0.2",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.0.1",
    "unified": "^10.1.1",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^4.0.0-beta.4",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.2.0",
    "eslint-config-next": "^12.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^2.4.1"
  }
}
